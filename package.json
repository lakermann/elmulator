{
  "name": "elmulator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "analyse": "npx elm-analyse -s -o",
    "run:watch": "npx elm-live src/Main.elm --open --start-page=index.html -- --output=elmulator.js",
    "test": "npx elm-test",
    "test:ci": "mkdir -p test-reports && npx elm-test --report=junit > ./test-reports/junit.xml",
    "test:watch": "npx elm-test --watch",
    "build:dev": "npx elm make src/Main.elm --output elmulator.js",
    "build:prod": "npx elm make src/Main.elm --output elmulator.js --optimize"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "elm": "^0.19.0-no-deps",
    "elm-analyse": "^0.16.4",
    "elm-format": "^0.8.2",
    "elm-live": "^4.0.1",
    "elm-test": "^0.19.0-rev6"
  }
}
